helpertype = Normal
name = "AI"
StateNo = 100000
ID = 10000+id
pos = 9999, 9999
keyctrl = 1
IgnoreHitPause = 1
pausemovetime = 9999
supermovetime = 9999
persistent = 0

[State -3, ヘルパー暴走の保険]
type = ChangeState
trigger1 = IsHelper
trigger1 = IsHelper(10000+Root,id)
trigger1 = StateNo != 100000
value = 100000
IgnoreHitPause = 1
SuperMoveTime = 9999
PauseMoveTime = 9999

;----------------------------------------------------------[ＡＩ起動セット]
[State -3, AIFlagSet]
type = VarSet
triggerall = !IsHelper
triggerall = Var(59) = 0
triggerall = NumHelper(20000+id)
triggerall = Helper(20000+id),Var(57) != -1
trigger1 = Command="AI1"||Command="AI2"||Command="AI3"||Command="AI4"||Command="AI5"
trigger2 = Command="AI6"||Command="AI7"||Command="AI8"||Command="AI9"||Command="AI10"
