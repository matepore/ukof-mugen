;*********************************************************************
; ヘルパーの暴走防止
;*********************************************************************
;===============================================================
; コマンド認識用ヘルパーの暴走防止
;===============================================================
[State -3, ChangeState]
type = ChangeState
trigger1 = IsHelper(8990)
value = 8990
ignorehitpause = 1


;*********************************************************************
; ヘルパーの呼び出し
;*********************************************************************
;===============================================================
; コマンド認識＆Var管理ヘルパーの呼び出し(Pause中も処理できる)
;===============================================================
[State -3, Helper]
type = Helper
trigger1 = NumHelper(8990) = 0
trigger1 = Alive = 1
trigger1 = !IsHelper
helpertype = Normal
name = "CommandHelper & Var"
stateno = 8990
ID = 8990
postype = p1
pos = 0,9999
keyctrl = 1
pausemovetime = 99999999999999
supermovetime = 99999999999999

;===============================================================
; 汎用常駐ヘルパーの呼び出し(Pause中は処理できない)
;===============================================================
[State -2, Helper]
type = Helper
trigger1 = NumHelper(8000) = 0
trigger1 = !IsHelper
name = "ResidentHelper"
StateNo = 8000
id = 8000
pos = 0,9999
postype = p1
keyctrl = 0
ownpal = 1
ignorehitpause = 1


;*********************************************************************
; イントロ音声消去
;*********************************************************************
