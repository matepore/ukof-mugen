;-----------------------------------------------------------------------------
;設定項目
;-----------------------------------------------------------------------------
[Statedef 40000]
anim = 9999
ctrl = 0

;-----------------------------------------------------------------------------
;本体システム設定
;-----------------------------------------------------------------------------
; MUGENバージョン設定
;  var(0) = 0 : MUGEN 1.0
;  var(0) = 1 : WinMugen

[State 40000, MUGEN Version]
Type = VarSet
Trigger1 = 1
var(0) = 1
;(0:MUGEN 1.0 1:WinMugen)

;---------------------------------------------------------------------------
; ADV／EXモード設定時間設定
;  var(1)の数値を任意の値に変更して下さい。単位は秒です。
;  デフォルトはvar(1) = 5(5秒)。

[State 40000, Mode Setting Time]
type = VarSet
trigger1 = 1
var(1) = 5 ;(*sec)

;-----------------------------------------------------------------------------
;AI設定
;-----------------------------------------------------------------------------
; AIレベル設定
;  var(57)の数値を1〜8の間で変更して下さい。

[State 40000, AI Level]
Type = ParentVarSet
trigger1 = 1
var(57) = 8
;(Please Setting between 1-8)

;---------------------------------------------------------------------------
; 反応速度設定
;  var(56)の数値を1〜10の間で変更して下さい。

[State 40000, AI Response]
Type = ParentVarSet
trigger1 = 1
var(56) = 10
;(Please Setting between 1-10)

;-----------------------------------------------------------------------------
;設定項目はここまで。以下はいじらないでください
;-----------------------------------------------------------------------------

[State 40000, AssertSpecial]
type = AssertSpecial
Trigger1 = 1
flag = NoShadow
flag2 = invisible

[State 40000, NotHitBy]
type = NotHitBy
Trigger1 = 1
value = SCA,AA,AT,AP

;[State 40000, DestroySelf]
;type = DestroySelf
;trigger1 = RoundState > 1

;この下、一行以上空ける事。
