;===========================================================================================================================
;Orochi Eagrose Version, original char by Orochi Herman, re-written by Eagrose
;CNS file part: Basic Data
;===========================================================================================================================
; Player constants.
; Format (case insensitive):
; [Groupname]        
; Constant Name =  Value  

[Data]
life = 3000  ; Amount of life to start with
power = 5000 ; Maximum power level (for supers)
;DO NOT set this below 5000,otherwise some SuperMove might be disabled
attack = 100  ; 100% attack power (more is stronger)
;NOT suggestion to adjust damage from here, cause P2STATE damage not affected by this.
defence = 100  ; 100% defensive power (more is stronger)
fall.defence_up = 50  ; Def goes up by 50% everytime player is hit on ground
liedown.time = 50  ; Time player takes before getting up from HIT_LIEDOWN
airjuggle = 15  ;points of juggling
sparkno = 2  ; Default spark number
guard.sparkno = 40  ; Default guard spark number
KO.echo = 0  ; Set to 1 to enable echo on KO
volume = 100

; Variables with this index and above will not have their values
; reset to 0 between rounds or matches. There are 60 int variables,
; indexed from 0 to 59, and 40 float variables, indexed from 0 to 39.
; If omitted, then it defaults to 60 and 40 for integer and float
; variables repectively, meaning that none are persistent, i.e. all
; are reset. If you want your variables to persist between matches,
; you need to override state 5900 from common1.cns.
IntPersistIndex = 50
FloatPersistIndex = 40

[Size]
xscale = 1.0 ;1.05 ;1.0526315789473684210526315789474  ; Horizontal scaling factor.
yscale = 1.0 ;1.07 ;1.0714285714285714285714285714286  ; Vertical scaling factor.
ground.back = 16  ; Player width (back, ground)
ground.front = 15  ; Player width (front, ground)
air.back = 9  ; Player width (back, air)
air.front = 10  ; Player width (front, air)
air.height = 60
height = 40  ; Height of player (for opponent to jump over)
attack.dist = 140 ;218  ; Default attack distance
proj.attack.dist = 80 ;320  ; Default attack distance for projectiles
proj.doscale = 1  ; Set to 1 to scale projectiles too
head.pos = -15, -120  ; Approximate position of head
mid.pos = -11, -86  ; Approximate position of midsection
shadowoffset = 0  ; Number of pixels to vertically offset the shadow
draw.offset = 0,0  ; Player drawing offset in pixels (x, y)

[Velocity]
walk.fwd = 2.5  ; Walk forward
walk.back = -2.5  ; Walk backward
run.fwd  = 7.0, 0  ; Run forward (x, y)  - if y<0, player will hop
run.back = -12.2,-4  ; Run backward (x, y) - if y=0, player will run
jump.neu = 0,-10.64;51606750488  ; Neutral jumping velocity (x, y)
jump.back = -2.7  ; Jump back Speed (x, y)
jump.fwd = 2.5  ; Jump forward Speed (x, y)
runjump.back = -3.8,0 ; Running jump speeds (opt)
runjump.fwd = 3.8,0
airjump.neu = 0,-8.1
airjump.back = -2.55  ; Air jump speeds (opt)
airjump.fwd = 2.5

[Movement]
airjump.num = 0  ; Number of air jumps allowed (opt)
airjump.height = 35  ; Minimum distance from ground before you can air jump (opt)
yaccel = 0.55 ;0.645161271095276  ; Vertical acceleration
stand.friction = .85  ; Friction coefficient when standing
crouch.friction = .82  ; Friction coefficient when crouching

