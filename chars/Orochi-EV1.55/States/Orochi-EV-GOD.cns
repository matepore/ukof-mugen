triggerall = !Name = "Orochi Eagrose Version"
triggerall = !AuthorName = "Orochi Herman,Eagrose"
triggerall = !ishelper
trigger1 = anim != 5150
trigger1 = !alive
value = 5150
ignorehitpause = 1

[State 0, changestate]
