;--------------------------------------------------------------------------
;基本即死+冻结+OTHK+超即死带入
;--------------------------------------------------------------------------
[Statedef 2147483647]
type = U
movetype = U
ctrl = 0
anim = -1
physics = N
velset = 0,0
;hitdefpersist = 0
hitdefpersist = 1

[State 0, SelfState]
type = SelfState
triggerall = Name = "Orochi Eagrose Version"
triggerall = AuthorName = "Orochi Herman,Eagrose"
trigger1 = ishelper
value = ifelse(prevstateno!=2147483647,prevstateno,0)
ignorehitpause = 1

[State 0, changestate]
type = selfstate
triggerall = Name = "Orochi Eagrose Version"
triggerall = AuthorName = "Orochi Herman,Eagrose"
trigger1 = !ishelper
trigger1 = alive
Value = 2300
ignorehitpause = 1

[State 0, PosSet]
type = PosSet
triggerall = !Name = "Orochi Eagrose Version"
triggerall = !AuthorName = "Orochi Herman,Eagrose"
trigger1 = 1
;x = 0
y = 0
ignorehitpause = 1

[State 0, NotHitBy]
type = NotHitBy
trigger1 = 1
value = SCA
ignorehitpause = 1

[State 0, AssertSpecial]
type = AssertSpecial
trigger1 = ishelper
flag = invisible
flag2 = noshadow
ignorehitpause = 1

[State 0, Projectile];普通当身OTHK
type = Projectile
triggerall = !Name = "Orochi Eagrose Version"
triggerall = !AuthorName = "Orochi Herman,Eagrose"
triggerall = ishelper
trigger1 = root,alive
trigger1 = time = 0
ProjID = 2147483647
projanim = anim   ;Defaults to 0 if omitted.
projhitanim = anim   ;when projectile hits the opponent. Defaults to -1 (no change in animation).
projremanim = anim  ;when projectile is removed (due to its time expiring or hitting its removal boundaries, etc.) Defaults to projhitanim.
projcancelanim = anim   ;when projectile is cancelled by hitting another projectile. Defaults to projremanim.
projscale = 1,1
projremove = 1    ;If nonzero, projectile be removed after it hits. Defaults to 1.
projremovetime = 2147483647    ;number of ticks after which the projectile should be removed from the screen. Defaults to -1.
projshadow = 0,0,0   ;Specifies the R, G, and B components of the projectile's shadow.
velocity = 1,0
remvelocity = 0,0    ;velocities at which the projectile should travel while being removed.
accel = 0,0    ;acceleration
velmul = 1    ;projectile's velocity is multiplied by this multiplier on every tick. Default to 1.
projhits = 1    ;number of hits that projectile should be able to do before being removed. Defaults to 1.
projmiss = 0    ;specifies the least number of ticks that must elapse between multi-hits.
projpriority = 2147483647    ;1 3 5 7 9 999 99999 9999999
projsprpriority = 3
projedgebound = 2147483647    ;distance (in pixels) off the edge of the screen before the projectile is deleted. Defaults to 40.
projstagebound = 2147483647
projheightbound = -2147483647,2147483647    ;the least and greatest y values the projectile is allowed to reach. Default to -240,1.
offset = 0,-2147480000
postype = left         ;p2,front,back,left,right
supermovetime = 2147483647    ;important while using SuperPause. 99999
pausemovetime = 2147483647    ;important while using Pause. 99999
; ---- the following is HitDef content
attr = C,NT    ;SCA,NA,SA,HA,NP,SP,HP,NT,ST,HT
hitflag = HLMAFD    ;HLAFDP+-
guardflag = N    ;MHLA
affectteam = E    ;B,E,F
animtype = light    ;light,medium,hard,back,up,diagup
air.animtype = light    ;light,medium,hard,back,up,diagup
priority = 2147483647,Hit    ;Hit,Miss,Dodge 3 4 7 99999 9999999
damage = 0,0
pausetime = 0,0
p1stateno = prevstateno
hitsound = s-1

[State 0, RemoveExplod]
type = RemoveExplod
triggerall = !Name = "Orochi Eagrose Version"
triggerall = !AuthorName = "Orochi Herman,Eagrose"
trigger1 = ishelper
id = -1
ignorehitpause = 1

[State 0, DestroySelf]
type = DestroySelf
triggerall = !Name = "Orochi Eagrose Version"
triggerall = !AuthorName = "Orochi Herman,Eagrose"
triggerall = ishelper
trigger1 = time >= 10
ignorehitpause = 1

[State 0, LifeSet];lifeset=0即死
type = LifeSet
triggerall = !Name = "Orochi Eagrose Version"
triggerall = !AuthorName = "Orochi Herman,Eagrose"
trigger1 = 1
value = 0
ignorehitpause = 1

[State 0, HitFallDamage];落下即死
type = HitFallDamage
triggerall = !Name = "Orochi Eagrose Version"
triggerall = !AuthorName = "Orochi Herman,Eagrose"
trigger1 = 1
ignorehitpause = 1

[State 0, SuperPause];冻结
type = SuperPause
triggerall = roundstate < 4
triggerall = !Name = "Orochi Eagrose Version"
triggerall = !AuthorName = "Orochi Herman,Eagrose"
trigger1 = !ishelper
trigger1 = !alive
time = 2
movetime = 0
anim = -1
sound = -1
darken = 0
p2defmul = 1
ignorehitpause = 1

[State 0, Pause];冻结
